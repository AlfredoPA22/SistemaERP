        function MostrarMensaje(msj) {
            $("#modalEliminar").modal('hide');
            $("#mensaje").show();
            $("#mensaje").html(msj);

        }

        function MostrarMensajeModal(msj) {

            $("#mensajeModal").show();
            $("#mensajeModal").html(msj);

        }

        function MostrarMensajeExito(msj) {
            $("#mensajeModal").hide();
            listarEmpresa();
            var msjbody = "Se Modifico los Datos Exitosamente";
            if ($("#itIdEmpresa").val().length === 0) {
                msjbody = "Se Registro los Datos Exitosamente";
            }

            $(document).Toasts('create', {
                class: 'bg-success',
                title: msj,
                autohide: true,
                delay: 10000,
                body: msjbody
            });
            $("#itIdEmpresa").val("");
            $("#itNombre").val("");
            $("#itNit").val("");
            $("#itSigla").val("");
            $("#itTelefono").val("");
            $("#itCorreo").val("");
            $("#itDireccion").val("");
            $("#modalEmpresa").modal('hide');
            console.log(msj);
        }


        function MostrarMensajeEliminacion(msj) {
            $("#mensaje").hide();
            listarEmpresa();
            var msjbody = "Se elimino la empresa";


            $(document).Toasts('create', {
                class: 'bg-success',
                title: msj,
                autohide: true,
                delay: 10000,
                body: msjbody
            });

            $("#modalEliminar").modal('hide');
            console.log(msj);
        }</script>
